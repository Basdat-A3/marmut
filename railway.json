{
    "providers": [],
    "buildImage": "ghcr.io/railwayapp/nixpacks:ubuntu-1699920194",
    "variables": {
      "NIXPACKS_METADATA": "python2.7,django,postgres",
      "PYTHONUNBUFFERED": "1"
    },
    "phases": {
      "install": {
        "dependsOn": ["setup"],
        "cmds": [
          "pip install virtualenv",
          "virtualenv /opt/venv",
          ". /opt/venv/bin/activate",
          "pip install -r requirements.txt"
        ],
        "cacheDirectories": ["/root/.cache/pip"],
        "paths": ["/opt/venv/bin"]
      },
      "setup": {
        "nixPkgs": ["python2.7", "postgresql", "gcc"],
        "nixLibs": ["zlib", "stdenv.cc.cc.lib"],
        "nixOverlays": [],
        "nixpkgsArchive": "5148520bfab61f99fd25fb9ff7bfbb50dad3c9db"
      }
    },
    "deploy": {
      "startCommand": "python2.7 manage.py migrate && python2.7 manage.py collectstatic --noinput && gunicorn README.wsgi",
      "restartPolicyType": "ON_FAILURE",
      "restartPolicyMaxRetries": 10
    }
  }
  